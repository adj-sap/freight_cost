{
  "_version": "1.59.0",
  "sap.app": {
    "id": "freight",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.12.1",
      "toolsId": "524a04d2-8b5b-4e97-a8b9-5a4562a02737"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/cost/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.4",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "freight.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "FreightCostsList",
          "target": "FreightCostsList"
        },
        {
          "pattern": "FreightCosts({key}):?query:",
          "name": "FreightCostsObjectPage",
          "target": "FreightCostsObjectPage"
        },
        {
          "pattern": "FreightCosts({key})/_costItems({key2}):?query:",
          "name": "CostItemsObjectPage",
          "target": "CostItemsObjectPage"
        }
      ],
      "targets": {
        "FreightCostsList": {
          "type": "Component",
          "id": "FreightCostsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/FreightCosts",
              "variantManagement": "Page",
              "navigation": {
                "FreightCosts": {
                  "detail": {
                    "route": "FreightCostsObjectPage"
                  }
                }
              },
              "initialLoad": "Enabled",
              "defaultTemplateAnnotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#table",
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "FreightCostsObjectPage": {
          "type": "Component",
          "id": "FreightCostsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/FreightCosts",
              "navigation": {
                "_costItems": {
                  "detail": {
                    "route": "CostItemsObjectPage"
                  }
                }
              }
            }
          }
        },
        "CostItemsObjectPage": {
          "type": "Component",
          "id": "CostItemsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/FreightCosts/_costItems"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
